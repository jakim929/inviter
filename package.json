{
  "name": "@evmts/example-vite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "// typecheck": "tsc --noEmit",
    "anvil": "source .env && anvil --fork-url $RPC_URL",
    "build": "vite build",
    "build:contracts": "forge build",
    "clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
    "dev": "source .env && vite",
    "devsetup": "./devsetup.sh",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "preinstall": "npx only-allow pnpm",
    "lint": "rome check . --apply-unsafe",
    "lint:check": "rome check . --verbose",
    "package:up": "pnpm up --latest",
    "preview": "vite preview",
    "rome": "^12.1.2",
    "serve": "serve -s -l tcp://0.0.0.0:5173 dist",
    "serve:test": "source .env & anvil --fork-url $RPC_URL & vite",
    "test": "vitest"
  },
  "dependencies": {
    "@eth-optimism/contracts-bedrock": "^0.16.0",
    "@evmts/core": "^0.5.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@rainbow-me/rainbowkit": "^1.0.7",
    "@safe-global/safe-apps-provider": "^0.17.1",
    "@safe-global/safe-apps-sdk": "^8.0.0",
    "@stablelib/random": "^1.0.2",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "ds-test": "github:dapphub/ds-test",
    "lucide-react": "^0.263.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "util": "^0.12.5",
    "viem": "^1.4.0",
    "wagmi": "^1.3.9",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@evmts/bundler": "^0.5.7",
    "@evmts/config": "^0.5.6",
    "@evmts/ts-plugin": "^0.5.7",
    "@evmts/vite-plugin": "^0.5.7",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "abitype": "^0.9.3",
    "autoprefixer": "^10.4.14",
    "forge-deploy": "^0.2.0",
    "forge-std": "github:foundry-rs/forge-std",
    "postcss": "^8.4.27",
    "react-helmet": "^6.1.0",
    "rome": "^12.1.3",
    "serve": "^14.2.0",
    "shadcn-ui": "^0.3.0",
    "solc": "0.8.15",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  }
