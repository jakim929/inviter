                "420": "0x2335022c740d17c2837f9C884Bfe4fFdbf0A95D5"
              }
            },
            {
              "name": "AttestationStation",
              "addresses": {
                "10": "0xEE36eaaD94d1Cc1d0eccaDb55C38bFfB6Be06C77",
                "420": "0xEE36eaaD94d1Cc1d0eccaDb55C38bFfB6Be06C77"
